<%

	# Generate a list of LI elements to show in ONE SWIMLANE of the timeline.
	# At time of writing this was called by trips/_timeline.html.erb

	elements					||= @elements
	isOverview				||= @isOverview || false	# Depricated
	selected_element	||= @selected_element
	
elements.each do |element| -%>
	<%= partial "trip_elements/show", :element => element, :isOverview => isOverview, :selected => ( element == selected_element ) %>
<% end -%>